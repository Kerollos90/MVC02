@using Company.Service.Interface.DepartmenInterface.Dto
@model DepartmentDto
@{
    ViewData["Title"] = "Update";
}

<h1>Update Department</h1>
<br />
<br />

<form asp-action="Update">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Code"></label>
        <input type="Text" asp-for="Code" class="form-control" />
          <span asp-validation-for="Code" class="text-danger"></span> 
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input type="Text" asp-for="Name" class="form-control" />
           <span asp-validation-for="Name" class="text-danger"></span> 
    </div>

    <input type="submit" value="Save" class="btn btn-primary" />
    <a asp-action="Index" class=" btn btn-light">Back To List</a>

</form>





