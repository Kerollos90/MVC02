@using Company.Service.Interface.EmployeeInterface.Dto
@model ApplicationUser
@{
    ViewData["Title"] = "Details";
}

<h1>User Details</h1>
<br />
<br />

<form asp-action="Edit" asp-route-id="@Model.Id">
    <div class="form-group">
        <label class="col-form-label" asp-for="Id"></label>
        <input type="Text" asp-for="Id" class="form-control" readonly />
        @*  <span asp-validation-for="Code" class="text-danger"></span> *@
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="UserName"></label>
        <input type="Text" asp-for="UserName" class="form-control" readonly />
        @*  <span asp-validation-for="Code" class="text-danger"></span> *@
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Email"></label>
        <input type="Text" asp-for="Email" class="form-control" readonly />
        @*   <span asp-validation-for="Name" class="text-danger"></span> *@
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="FirstName"></label>
        <input type="Text" asp-for="FirstName" class="form-control" readonly />
        @*   <span asp-validation-for="Name" class="text-danger"></span> *@
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="LastName"></label>
        <input type="Text" asp-for="LastName" class="form-control" readonly />
        @*   <span asp-validation-for="Name" class="text-danger"></span> *@
    </div>
   
    <br />


    <a asp-action="Index" class=" btn btn-secondary">Back To List</a>

</form>






